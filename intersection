// create a function named intersection that 
// takes 2 arrays of integers as input
// and returns an array that represents the intersection of the 2 arrays

// for example with [1,2,3] e [4,5,6] it returns []
// for example with [1,2] e [2,3] it returns [2]

function intersection(a, b) {
  let result = [];
  for (let i = 0; i < a.length; i++) {
    if (b.indexOf(a[i]) != -1) {
      result.push(a[i]);
    }
  }
  return result;
}
